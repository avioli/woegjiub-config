set defsearch=ddg
set editor="urxvt -e vim"
set gui=none
set rtp=$XDG_CONFIG_HOME/vimperator
set sb=false
colo darkness
ignorekeys add -except=d,s,S,b,:,g,t,T,o,O .*woegjiub.com.*

map d <C-w>
noremap a a -tags 
map m a
map A O
map s o ddg 
map S t ddg 
map <C-v> <S-Esc>
map <A-Left> <C-S-Tab>
map <A-Right> <C-Tab>
map <A-Up> b
map <A-Down> t
map <C-S-Left> H
map <C-S-Right> L
